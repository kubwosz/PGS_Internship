<Page x:Class="PGS_Internship_Jakub_Woszczyna.Pages.PagePhoneNumber"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PGS_Internship_Jakub_Woszczyna.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PagePhoneNumber">

    <Grid>
        <Button x:Name="buttonFinish" Content="Zakończ" HorizontalAlignment="Left" Margin="603,381,0,0" VerticalAlignment="Top" Width="75" Click="buttonFinish_Click"/>
        <Button x:Name="buttonPrev" Content="Powrót" HorizontalAlignment="Left" Margin="137,381,0,0" VerticalAlignment="Top" Width="75" Click="buttonPrev_Click"/>
        <Label x:Name="labelPhoneNumber" Content="Podaj swój numer telefonu:" HorizontalAlignment="Center" Margin="0,147,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxPhoneNumber" HorizontalAlignment="Center" Height="23" Margin="0,173,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="150" GotFocus="textBoxPhoneNumber_GotFocus"/>

    </Grid>
